@{
    ViewData["Title"] = "Mission 3 - calculator";
}

@model GradeCalculatorModel;

<h2>Grade Calculator</h2>
<form method="post">
    <label>Assignments: </label>
    <input id="assign" asp-for="Assign" /> <br />

    <label>Group Project: </label>
    <input id="gp" asp-for="Gp" /> <br />

    <label>Quizzes: </label>
    <input id="qz" asp-for="Qz" /> <br />

    <label>Exams: </label>
    <input id="ex" asp-for="Ex" /> <br />

    <label>INTEX: </label>
    <input id="intex" asp-for="Intex" /> <br />

    <button id="btnsend" type="submit" value="Check Grade" />

    <div asp-validation-summary="All" class="text-danger"></div>
</form>


